<?xml version="1.0" encoding="utf-8"?>
<Window
    x:Class="SystemAI_LR.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SystemAI_LR"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="SystemAI_LR">

    <Window.SystemBackdrop>
        <MicaBackdrop />
    </Window.SystemBackdrop>
    <StackPanel>
        <TitleBar x:Name="titleBar" Title="Лабораторные работы по дисциплине &quot;Системы искусственного интеллекта &quot;" >
            <TitleBar.IconSource>
                <SymbolIconSource Symbol="View"/>
            </TitleBar.IconSource>
        </TitleBar>
        <TabView CanReorderTabs="False" CanTearOutTabs="False" IsAddTabButtonVisible="False" >
            <TabViewItem Header="Лабораторная 1" IsClosable="False">
                <local:LR1/>
            </TabViewItem>
            <TabViewItem Header="Лабораторная 2" IsClosable="False">
                <local:LR2/>
            </TabViewItem>

        </TabView>
    </StackPanel>
</Window>
